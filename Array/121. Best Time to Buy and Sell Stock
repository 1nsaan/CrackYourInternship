class Solution {
    public int maxProfit(int[] prices) {
        int cp=prices[0],maxp=0;
          for(int i=1;i<prices.length;i++){
              if(prices[i]<cp){
                  cp = prices[i];
                  
              }else{
                  maxp = (Math.max(maxp, prices[i]-cp));
              }
          }
        return maxp;
    }
}
