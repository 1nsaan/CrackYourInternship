class Solution {
    public void moveZeroes(int[] nums) {
        if(nums.length==1) return;
        int nz=0,temp=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]!=0){
              temp=nums[nz];
              nums[nz]=nums[i];
              nums[i]=temp;
              nz++;  
              }
            }
    }
}
}
